{
    "name": "nlappe/LaravelActivityLogExtender",
    "type": "library",
    "description": "A small trait that offers additional generic functions to log and aggregate ActivityLogs written by spatie/laravel-activitylog. It uses spatie/Enum to fixate Log types. Its usefull for Statistics.",
    "keywords": ["spatie","ActivityLog","Statistics","aggregate", "Trait"],
    "homepage": "https://github.com/nlappe",
    "license": "MIT",
    "authors": [
        {
            "name": "Nils Lappe",
            "homepage": "https://github.com/nlappe",
            "role": "Developer"
        }
    ],
        "require": {
        "php": ">=7.3",
        "spatie/laravel-activitylog": "^3.5",
        "spatie/enum": "^2.1"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "nlappe\\LaravelActivityLogExtender\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}